{
    "name": "bpedroza/laravel-mail-assertions",
    "description": "Make assertions on mail in laravel.",
    "keywords": ["laravel", "testing", "mail", "email"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bryan Pedroza"
        }
    ],
    "type": "project",
    "require": {
        "php": "^7.2",
        "illuminate/support": "~5.5|^6.0|^7.0|^8.0",
        "illuminate/mail": "~5.5|^6.0|^7.0|^8.0",
        "illuminate/view": "~5.5|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~8.0"
    },
    "autoload": {
        "psr-4": {
            "Bpedroza\\LaravelMailAssertions\\": "src/LaravelMailAssertions"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
